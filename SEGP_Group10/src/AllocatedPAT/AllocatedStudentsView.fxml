<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<Tab fx:id="tab" text="Allocated Students" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AllocatedPAT.AllocatedPATsController">
   <content>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <ScrollPane BorderPane.alignment="CENTER">
               <content>
                  <VBox fx:id="vBox" spacing="10.0" stylesheets="@TreeTableView.css" />
               </content>
            </ScrollPane>
         </center>
         <bottom>
            <HBox alignment="BOTTOM_RIGHT" BorderPane.alignment="CENTER">
               <children>
                  <JFXButton onMouseClicked="#extractPDFOnClicked" prefWidth="150.0" text="Extract PDF">
                     <padding>
                        <Insets bottom="10.0" right="20.0" top="10.0" />
                     </padding>
                  </JFXButton>
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </content>
</Tab>
